            <!-- Log Module - Interceptor Module -->
            <dependency>
                <groupId>com.ppxb</groupId>
                <artifactId>latte-starter-log-interceptor</artifactId>
                <version>${revision}</version>
            </dependency>

            <!-- Web Module -->
            <dependency>
                <groupId>com.ppxb</groupId>
                <artifactId>latte-starter-web</artifactId>
                <version>${revision}</version>
            </dependency>

            <!-- Security Module - Crypto -->
            <dependency>
