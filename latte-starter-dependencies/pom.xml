                <scope>import</scope>
            </dependency>

            <!-- FIXME: CVE ISSUES SO THE DEPENDENCY SHOULD BE REPLACED -->
            <!-- TLog -->
            <dependency>
                <groupId>com.yomahub</groupId>
                <artifactId>tlog-web-spring-boot-starter</artifactId>
